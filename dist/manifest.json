{
  "manifest_version": 3,
  "name": "GithubEditorIndentExtension",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
        "inject.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "GithubEditorIndentExtension",
    "default_popup": "setting.html",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    }
  },
  "permissions": [
    "storage"
  ]
}